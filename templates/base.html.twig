        {{ encore_entry_script_tags('js/dataTables.fixedColumns') }}
        {{ encore_entry_script_tags('js/dataTables.fixedHeader') }}
        {{ encore_entry_script_tags('js/dataTables.colReorder') }}
        {{ encore_entry_script_tags('js/jszip.min') }}


        {#HIGHCHARTS#}
