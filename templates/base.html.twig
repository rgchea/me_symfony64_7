    <!-- Head Libs -->
    {#<script src="{{ asset('css/portoadmin/vendor/modernizr/modernizr.js') }}"></script>#}

</head>
<body>
{% block body %}


    <section class="body">
        <!-- start: header -->
        {{ include('Partials/header.html.twig') }}
        <!-- end: header -->
        <div class="inner-wrapper">
            <!-- start: navigation bar -->

            <section role="main" class="content-body">

                <header class="page-header">

                    <!-- start: search & user box -->
                    <div class="header-right">
                        {#
                        <form action="pages-search-results.html" class="search nav-form">
                            <div class="input-group">
                                <input type="text" class="form-control" name="q" id="q" placeholder="Search...">
                                <span class="input-group-append">
                                                <button class="btn btn-default" type="submit"><i class="fas fa-search"></i></button>
                                            </span>
                            </div>
                        </form>

                        #}

                        <span class="separator"></span>

                        {{ include('Partials/header_notifications.html.twig') }}

                        <span class="separator"></span>

                        {{ include('Partials/header_user.html.twig') }}
                    </div>
                    <!-- end: search & user box -->

                    <div class="right-wrapper text-right">
                        <ol class="breadcrumbs">
                            <li>
                                <a href="{{ path('backend_admin_homepage') }}">
                                    <i class="fas fa-home"></i>
                                </a>
                            </li>
                            {% block breadcrumb %}
                                <li><span>HOME</span></li>
                            {% endblock %}

                        </ol>

                    </div>

                    {% for flashMessage in app.session.flashbag.get('warning') %}

                        <div class="alert alert-warning">
                            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                            <strong>{{ 'label_alert.warning'|trans }}</strong> {{ flashMessage|raw }}
                        </div>

                    {% endfor %}


                    {% for flashMessage in app.session.flashbag.get('success') %}

                        <div class="alert alert-success">
                            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                            <strong>{{ 'label_alert.success'|trans }}</strong> {{ flashMessage|raw }}
                        </div>
                    {% endfor %}

                    {% for flashMessage in app.session.flashbag.get('info') %}
                        <div class="alert alert-info">
                            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                            <strong>{{ 'label_alert.info'|trans }}</strong> {{ flashMessage |raw }}
                        </div>

                    {% endfor %}

                    {% for flashMessage in app.session.flashbag.get('danger') %}

                        <div class="alert alert-danger">
                            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                            <strong>{{ 'label_alert.danger'|trans }}</strong> {{ flashMessage|raw }}
                        </div>

                    {% endfor %}


                    {% block page_header %}{% endblock %}

                </header>

                <!-- start: page -->
                <div class="row">
                    <div id="pizote_spinner" class="pizote_spinner"></div>
                    <div class="card-body">
                        <div class="col-md-12">

                            {% block myContent %}


                            {% endblock %}
                        </div>
                    </div>
                </div>

                <!-- end: page -->



                <!-- end: navigation bar -->

            </section>
        </div>
    </section>

{% endblock %}

</body>
</html>
