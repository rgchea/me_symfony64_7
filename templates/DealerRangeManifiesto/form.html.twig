					) }}
				</div>
				<div class="form-group col-md-6">
					{{ form_label(form.manifiestoTo) }}
					{{ form_errors(form.manifiestoTo) }}
					{{ form_widget(form.manifiestoTo, { 'attr': {'class' : 'form-control' } }
					) }}
				</div>
				<div class="form-group col-md-6">
					{{ form_label(form.active) }}
					{{ form_errors(form.active) }}
					{{ form_widget(form.active, { 'attr': {'class' : '' } }
					) }}
				</div>
