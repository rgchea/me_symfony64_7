						<i class="fa fa-save"></i>
						Guardar
					</button>
				</div>

{#                    </form>#}

			{{ form_end(form) }}

		</div>
		<!-- end widget content -->

	</div>
	<!-- end widget div -->


<script type="text/javascript" language="Javascript">

	$("[name='backend_adminbundle_dealerrangemanifiesto']").validate();
